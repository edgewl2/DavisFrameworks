{% extends 'layouts/app.html.twig' %}

{% block content %}
  <div class="row">
    <form action="/post" method="post" class="form-inline">
      <div class="input-group">
        <input type="text" class="form-control" name="form"/>
      </div>
      <div class="input-group">
        <input type="text" class="form-control" name="data"/>
      </div>
      <div class="btn-group">
        <input type="submit" class="btn btn-danger" value="Enviar Post"/>
      </div>
    </form>
  </div>
{% endblock %}